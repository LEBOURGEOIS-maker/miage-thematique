{% extends 'base.html.twig' %}

{% block title %}CHRONOS ADMIN - Dashboard{% endblock %}
{% block stylesheetsAdmin %}
<link rel="stylesheet" href={{ asset('css/admin.css') }}>
<link rel="stylesheet" href={{ asset('css/sb-admin-2.min.css') }}>
{% endblock %}

{% block body %}
<div id="wrapper">

  <div id="content-wrapper" class="d-flex flex-column">
    <div id="content">

      <div class="container-fluid">
        {% include 'cardsJustificationsAbsencesEtudiants.twig' %}
        <div class="card shadow mb-4">
          <div class="card-body">
              <div class="table-responsive">
                  {% include 'etudiantTableauJustificationsAbsences.twig' %}
              </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Footer -->
    {% include 'footer.html.twig' %}
  </div>
</div>
<a class="scroll-to-top rounded" href="#page-top">
  <i class="fas fa-angle-up"></i>
</a>
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('js/initEtudiantJustif.js') }}"></script>
    <script src="{{ asset('js/sb-admin-2.min.js') }}"></script>
{% endblock %}
