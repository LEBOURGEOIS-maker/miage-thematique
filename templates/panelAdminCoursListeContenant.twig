{% extends 'base.html.twig' %}

{% block title %}CHRONOS ADMIN - Dashboard{% endblock %}
{% block stylesheetsAdmin %}
<link rel="stylesheet" href={{ asset('css/admin.css') }}>
<link rel="stylesheet" href={{ asset('css/sb-admin-2.min.css') }}>
{% endblock %}

{% block body %}
<div id="wrapper">
  {% include 'sidebar.html.twig' %}
  <div id="content-wrapper" class="d-flex flex-column">
    <div id="content">
      <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
        <h4 class="mb-0 text-gray-800">
          Liste des <span class="text-info">cours</span> 
        </h4>
        {% include 'topbar.html.twig' %}
      </nav>
      <div class="container-fluid">
        {% include 'cardsListeAdminCours.twig' %}
        <div class="card shadow mb-4">
          <div class="card-body">
              <div class="table-responsive">
                  {% include 'panelAdminCoursListe.twig' %}
              </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Footer -->
    {% include 'footer.html.twig' %}
  </div>
</div>
<a class="scroll-to-top rounded" href="#page-top">
  <i class="fas fa-angle-up"></i>
</a>
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('js/initAdminAbsences.js') }}"></script>
    <script src="{{ asset('js/sb-admin-2.min.js') }}"></script>
{% endblock %}
